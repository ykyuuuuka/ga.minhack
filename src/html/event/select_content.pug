- var event_type = 'select_content'

doctype html
htlm(lang='ja')
	head
		title #{event_type}
		include ../head.pug
	body
		include ../body_head.pug
		.container
				form(name='form')
					.cnt-code
						.row
							.col-12.pb-3 dataLayer.push({
							.col-12.pb-3 　　　event: #{event_type},
						.row.align-items-center.pb-3
							.col-auto 　　　content_type:
							.col-auto.ps-0
								input.form-control(name='value1' value='123')
							.col-auto ,
						.row.align-items-center
							.col-auto 　　　item_id:
							.col-auto.ps-0
								input.form-control(name='value2' value='123')
						.row
							.col-12.pb-3 });
							.col-3.d-grid
								p.m-0.btn.btn-primary(data-js=event_type) submit
						include ../submit_success.pug						
		include ../event_menu.pug